{
	"name": "cartalyst/themes",
	"description": "A theme framwork for your next web application. Theme inheritence, powered by Laravel and Assetic, can be used in any application!",
	"keywords": ["platform", "ecommerce", "laravel", "bootstrap"],
	"license": "BSD-3-Clause",
	"authors": [
		{
			"name": "Cartalyst LLC",
			"email": "help@cartalyst.com"
		},
		{
			"name": "Ben Corlett",
			"email": "ben.corlett@cartalyst.com",
			"role": "Developer"
		},
		{
			"name": "Bruno Gaspar",
			"email": "bruno.gaspar@cartalyst.com",
			"role": "Developer"
		},
		{
			"name": "Dan Syme",
			"email": "dan.syme@cartalyst.com",
			"role": "Project Lead"
		}
	],
	"repositories": [
		{
			"type": "composer",
			"url": "http://packages.cartalyst.com"
		}
	],
	"require": {
		"php": ">=5.3.0",

		"herrera-io/json": "1.0.*",
		"kriswallsmith/assetic": "1.1.*",
		"jasonlewis/resource-watcher": "1.0.*",

		"cartalyst/dependencies": "1.0.*",

		"illuminate/filesystem": "4.0.*",
		"illuminate/support": "4.0.*",
		"illuminate/view": "4.0.*"
	},
	"suggest": {
		"cartalyst/assetic-filters": "CoffeeScript & SASS PHP compiler.",
		"jasonlewis/basset": "URL rewrite filter.",
		"leafo/lessphp": "LESS PHP compiler.",
		"leafo/scssphp": "SCSS PHP compiler.",
		"leafo/scssphp-compass": "Compass support for SCSS PHP compiler.",
		"natxet/CssMin": "CSS PHP minification.",
		"lmammino/jsmin4assetic": "JavaScript PHP minification."
	},
	"require-dev": {
		"mockery/mockery": "0.7.2",

		"illuminate/events": "4.0.*",
		"illuminate/routing": "4.0.*",
		"illuminate/view": "4.0.*"
	},
	"autoload": {
		"psr-0": {
			"Cartalyst\\Themes": "src"
		}
	},
	"extra": {
		"component": "package",
		"frameworks": ["Laravel 3", "Laravel 4"],
		"versions": {
			"1": "1.0/master",
			"2": "master"
		},
		"branch-alias": {
			"dev-master": "2.0-dev"
		}
	},
	"minimum-stability": "dev"
}
